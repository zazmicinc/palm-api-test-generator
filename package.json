{
  "name": "palm-api-test-generator",
  "displayName": "PaLM API Unit Test Generator",
  "description": "Easily generate high-quality unit tests for your code using the power of the PaLM Language Model. This extension empowers developers to automate the creation of unit tests, ensuring code quality and reliability.",
  "version": "0.0.9",
  "publisher": "Zazmic",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zazmicinc/palm-api-test-generator.git"
  },
  "icon": "icon.jpg",
  "galleryBanner": {
    "color": "#0b0009",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Testing",
    "Programming Languages",
    "Debuggers",
    "Machine Learning"
  ],
  "keywords": [
    "tests",
    "unit tests",
    "auto tests",
    "ai",
    "machine learning",
    "palm api",
    "test framework",
    "jest",
    "mocha",
    "chai",
    "enzyme",
    "testng",
    "phpunit",
    "rspec",
    "cucumber",
    "jasmine",
    "junit",
    "nunit",
    "pytest",
    "qunit",
    "cypress",
    "ava",
    "protractor",
    "ginkgo",
    "robot framework",
    "playwright",
    "selenium",
    "karma"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "menus": {
      "editor/context": [
        {
          "command": "palm-api-test-generator.GenerateUnitTest"
        }
      ]
    },
    "commands": [
      {
        "command": "palm-api-test-generator.GenerateUnitTest",
        "title": "PaLM API: Generate Unit Test"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}
